	shortjournal = {Genome Biology and Evolution},
	author = {Sun, Cheng and Shepard, Donald B. and Chong, Rebecca A. and López Arriaza, José and Hall, Kathryn and Castoe, Todd A. and Feschotte, Cédric and Pollock, David D. and Mueller, Rachel Lockridge},
